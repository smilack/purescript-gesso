{
  "name": "purescript-gesso",
  "version": "1.0.0",
  "description": "A HTML canvas wrapper in PureScript for creating graphical standalone or Halogen applications",
  "scripts": {
    "example-all": "",
    "example-clock": "spago --config examples/clock/spago.dhall --main Example.clock.Main --to examples/clock/dist/example.js",
    "example-unit-grid": "spago --config examples/unit-grid/spago.dhall --main Example.unit-grid.Main --to examples/unit-grid/dist/example.js",
